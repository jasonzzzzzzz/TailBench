source $script_dir/vol.init

set nrec 2000
set nrec 10

if {$logging==0} {
    echo "Logging off: script not run. "
} else {
    # set nulls to one of { notnull nullok }
    set nulls nullok

    sm begin_xct
	verbose tid [sm xct_to_tid [sm xct]]
	verbose sm test_bulkload_rtree $volid $nrec $nulls
	sm test_bulkload_rtree $volid $nrec $nulls
	set n [sm gather_xct_stats reset]

	# stats
	verbose $n
    sm commit_xct

    unset n  nulls
}

unset nrec 

